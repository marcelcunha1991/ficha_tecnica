<%- include ('../partials/header.ejs') %>
<%- include ('../partials/sidebar.ejs') %>

<div class="container-fluid"> 

   <hr>
   <h2>Fichas Técnicas</h2> 
   <div id="nav">
      <a class="btn btn-success" href="/novaficha"> Inserir Nova Ficha Técnica</a>
      <a class="btn btn-success" href="/ficha/lista"> Lista de Fichas Técnicas</a>
   </div>
   <hr> 

   <select name="maquinas" id="maquinas"  class="form-control">
      <option value="0"> Selecione uma máquina</option>
      <% maquinas.forEach((maquina) => { %>
         <option value="<%= maquina.id %>"> <%= maquina.descricao %></option>
      <% }) %>
   </select>

   <br>

   <h5>Parâmetros De Programa : </h5>
   
   <div class="d-flex flex-column ficha">
      <div class="d-flex flex-row">
         <div style="width: 10px; height: 10px; background-color: blue; margin-right: 5px;"></div>Valores cadastrados
      </div>
      <div class="d-flex flex-row">
         <div style="width: 10px; height: 10px; background-color: green; margin-right: 5px;"></div>Valores reais
      </div>
   </div>

   <br>
   <table class="table table-bordered" id="parametros"/> 
      <thead>
         <!-- <tr>
            <th> Parâmetro </th>
            <th> Valor Mínimo </th>
            <th> Valor Máximo </th>
            <th> Último valor setado </th>             
         </tr> -->
      </thead>
      <tbody>
      </tbody>
   </table>


   <div id="myModal" class="modal fade" role="dialog">
      <div class="modal-dialog">
         <!-- Modal content-->
         <div id="plotly">
      </div>
      </div>
   </div>

   <div class="table-responsive ficha">
      <table class="table-bordered ficha" style="width: 100%; margin-bottom: 40px;">
         <thead>
            <tr>
               <th>Número do Molde:</th>
               <th>Número da Máquina:</th>
               <th>Cliente:</th>
               <th>Código PAM:</th>
               <th>Técnico:</th>
               <th>Produto:</th>
               <th>Material:</th>
               <th>Revisão:</th>
            </tr>
         </thead>
         <tbody>
            <tr>
               <td>
                  <input class="read-only" readonly style="width: 100%" id="molde" name="molde" value="<%= injetor.NúmeroMolde %>">
               </td>
               <td>
                  <input class="read-only" readonly style="width: 100%" id="numMaquina" name="numMaquina" value="<%= injetor.NúmeroMáquina %>">
               </td>
               <td>
                  <input class="read-only" readonly style="width: 100%" id="cliente" name="cliente" value="<%= injetor.Cliente %>">
               </td>
               <td>
                  <input class="read-only" readonly style="width: 100%" id="codigoPAM" name="codigoPAM" value="<%= injetor.CodigoPAM %>">
               </td>
               <td>
                  <input class="read-only" readonly style="width: 100%" id="tecnico" name="tecnico" value="<%= injetor.Tecnico %>">
               </td>
               <td>
                  <input class="read-only" readonly style="width: 100%" id="produto" name="produto" value="<%= injetor.Produto %>">
               </td>
               <td>
                  <input class="read-only" readonly style="width: 100%" id="material" name="material" value="<%= injetor.Material %>">
               </td>
               <td>
                  <input class="read-only" readonly style="width: 100%" id="material" name="material" value="<%= injetor.revisao %>">
               </td>
            </tr>
         </tbody>
      </table>
   </div>

