<%- include ('../partials/header.ejs') %>
<%- include ('../partials/sidebar.ejs') %>

<div class="container-fluid">
   <br>
   <form method="GET" action="/ficha/lista" style="display: inline;">
      <button class="btn btn-info" style="width: 70px;">
         <i class="fa fa-arrow-left" title="Voltar"></i>
      </button>
   </form>
   <br>
   <br>
   <div class="card">
      <div class="card-header">                
         <h2>Editar Máquina</h2>              
      </div>
         <div class="card-body">
            <form name="pamForm" method="POST" action="/fichas/updateHaitian" style="display: inline;" onsubmit="return validateForm()">
               <input type="hidden" id="id" name="id" value="<%= injetor.id %>">
               <input type="hidden" name="maquina" value="<%= injetor.maq %>">

               <div class="table-ficha table-responsive">
                  <table class="table-bordered" style="width: 100%; margin-bottom: 40px;">
                     <thead>
                        <tr>
                           <th><span style="color: red;">*</span>Número do Molde:</th>
                           <th>Número da Máquina:</th>
                           <th>Cliente:</th>
                           <th>Código PAM:</th>
                           <th>Técnico:</th>
                           <th>Produto:</th>
                           <th><span style="color: red;">*</span>Material:</th>
                        </tr>
                     </thead>
                     <tbody>
                        <tr>
                           <td>
                              <input style="width: 100%" id="molde" name="molde" list="moldeList" value="<%= injetor.NúmeroMolde %>">
                              <datalist id="moldeList">
                                 <% moldes.forEach((molde) => { %>
                                    <option value="<%= molde.codigo %>"></option>
                                 <% }) %>
                              </datalist>  
                           </td>
                           <td>
                              <input style="width: 100%" id="numMaquina" name="numMaquina" value="<%= injetor.NúmeroMáquina %>">
                           </td>
                           <td>
                              <input style="width: 100%" id="cliente" name="cliente" value="<%= injetor.Cliente %>">
                           </td>
                           <td>
                              <input style="width: 100%" id="codigoPAM" name="codigoPAM" value="<%= injetor.CodigoPAM %>">
                           </td>
                           <td>
                              <input class="read-only" readonly style="width: 100%" id="tecnico" name="tecnico" value="<%= injetor.Tecnico %>">
                           </td>
                           <td>
                              <input style="width: 100%" id="produto" name="produto" value="<%= injetor.Produto %>">
                           </td>
                           <td>
                              <input style="width: 100%" id="material" name="material" list="materialList" value="<%= injetor.Material %>">
                              <datalist id="materialList">
                                 <% materiais.forEach((material) => { %>
                                    <option value="<%= material.codigo %>"></option>
                                 <% }) %>
                              </datalist>
                           </td>
                        </tr>
                     </tbody>
                  </table>
               </div>

               <div class="form-floating" style="margin-bottom: 50px;">
                  <!-- <small>Usuário: 
                  <input style="border: none; font-weight: 600;" readonly style="width: 100%" id="userLogado" name="userLogado"></small><br> -->
                  <label for="justificativa">Motivo da alteração:</label>
                  <textarea class="form-control" placeholder="Justifique a alteração" id="justificativa" name="justificativa"></textarea>
               </div>

