<script>
   function validateForm() {
      var molde = document.forms["pamForm"]["molde"].value;
      var material = document.forms["pamForm"]["material"].value;
      var justificativa = document.forms["pamForm"]["justificativa"].value;

      if (molde === "") {
         alert("O número do Molde deve ser informado.");
         return false;
      }
      if (material === "") {
         alert("O número do Material deve ser informado.");
         return false;
      }
      if (justificativa === "") {
         alert("A justificativa deve ser informada.");
         return false;
      }
   }

   document.getElementById("edit").disabled = true;
</script>