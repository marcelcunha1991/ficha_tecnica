                     <input checked type="radio" id="termopar" name="termopar" value="K">
                     <label for="termopar">K</label>
                  </td>
                  <td colspan="1">
                     <input type="radio" id="termopar" name="termopar" value="J">
                     <label for="termopar">J</label>
                  </td>
   
                  <th colspan="1">VOLTAGEM:</th>
                  <td colspan="1">
                     <input checked type="radio" id="voltagem" name="voltagem" value="220">
                     <label for="voltagem">220</label>
                  </td>
                  <td colspan="1">
                     <input type="radio" id="voltagem" name="voltagem" value="24">
                     <label for="voltagem">24</label>
                  </td>
               </tr>
            </tbody>
         </table>
         
         <table class="table-bordered" style="width: 100%; margin-top: 40px">
            <thead>
               <tr>
                  <th colspan="5">CÂMARA QUENTE</th>
                  <th colspan="2">TOL &#177; 10%</th>
               </tr>
            </thead>
            <tbody>
               <% for(var j = 0; j < 19; j++) { %>
                  <tr>
                     <% for(var i = 0; i < 7; i++) { %>
                        <% count++ %>
   
                        <% if(count <= 130) { %>
                           <td>
                              <input style="width: 100%" id="camara<%= i+1 %>" name="camara<%= i+1 %>" placeholder="<%= count %>">
                           </td>
                        <% } %>
                     <% } %>
                  </tr>
               <% } %>
            </tbody>
         </table>
         
         <table class="table-bordered" style="width: 100%; margin-top: 40px">
            <thead>
               <tr>
                  <th colspan="5">VALVE GATES</th>
                  <th colspan="2">TOL &#177; 10%</th>
               </tr>
               <tr>
                  <th colspan="7">SEQUENCIADOR</th>
               </tr>
            </thead>
            <tbody>
               <tr>
                  <th>BICO</th>
                  <th>DLY TIME</th>
                  <th>ACT TIME</th>
                  <th>BICO</th>
                  <th>DLY TIME</th>
                  <th>ACT TIME</th>
               </tr>
               <% count = 0 %>
               <% count2 = 20 %>
               <% for(var j = 0; j < 20; j++) { %>
                  <tr>
                     <!-- lado esquerdo -->
                     <% for(var i = 0; i < 1; i++) { %> 
                        <th>VG<%= j+1 %></th>
                     <% } %>
                     
                     <% for(var i = 0; i < 2; i++) { %>
                        <td>
                           <input style="width: 100%" id="VG1" name="VG1" placeholder="VG<%= j+1 %>">
                        </td>
                     <% } %>
                        
                     <!-- lado direito -->
                     <% for(var i = 0; i < 1; i++) { %>
                        <th>VG<%= ++count2 %></th>
                     <% } %>
                     
                     <% for(var i = 0; i < 2; i++) { %>
                        <td>
                           <input style="width: 100%" id="VG2" name="VG2" placeholder="VG<%= count2 %>">
                        </td>
                     <% } %>
                  </tr>
               <% } %>
            </tbody>
         </table>
   
         <table class="table-bordered" style="margin-top: 40px">
            <thead>
               <tr>
                  <th colspan="7">REFRIGERAÇÃO DO MOLDE</th>
               </tr>
            </thead>
            <tbody>
               <tr>
                  <th colspan="3" style="text-align: center;">LADO FIXO</th>
                  <th rowspan="3" style="text-align: center;">TOL<br> &#177;<br> 10%</th>
                  <th colspan="3" style="text-align: center;">LADO MÓVEL</th>
               </tr>
               <tr>
                  <td colspan="3">
                     <input style="width: 100%" id="rmladofixo1" name="rmladofixo1" placeholder="50C">
                  </td>
                  <td colspan="3">
                     <input style="width: 100%" id="rmladomovel1" name="rmladomovel1" placeholder="GELADA">
                  </td>
               </tr>
               <tr>
                  <td colspan="3">
                     <input style="width: 100%" id="rmladofixo2" name="rmladofixo2" placeholder="50C">
