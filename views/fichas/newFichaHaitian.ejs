               <tr>
                  <% for(var i = 0; i < 7; i++) { %>
                     <td>
                        <input style="width: 100%" id="cilindro<%= i+1 %>" name="cilindro<%= i+1 %>">
                     </td>
                  <% } %>
               </tr>
            </tbody>
         </table>
   
