      <!-- PERIFÉRICOS -->
      <div class="table-responsive table-ficha d-flex flex-column justify-content-between" style="width: 50vw;">
         <table class="table-bordered" style="width: 100%">
            <tbody>
               <tr>
                  <th colspan="1">TERMOPAR:</th>
