<%- include ('../partials/header.ejs') %>
<%- include ('../partials/sidebar.ejs') %>

<div class="container">

 
  <br>
    <div class="card">
            <div class="card-header">
                   
                        <h2>Novo Parâmetros de máquina</h2>
                    
            </div>
            <div class="card-body">
                    <form  method="POST" action="/parametrosReal/create">

                        <select name="maquinas" class="form-control">
                                <option > Selecione uma máquina</option>
                                <% maquinas.forEach((maquina) => { %>
                        
                                        <option value="<%= maquina.id %>"> <%= maquina.descricao %></option>
                        
                                <% }) %>
                        
                            </select>
                            
                            <br>
                            
                                <div>
                                        <label for="username"> Production Shot Min: </label>
                                        <input id="VI1_min" name="VI1_min" size="30" type="number" />
                                
                                        <label for="name">Production Shot Max: </label>
                                        <input id="VI1_max" name="VI1_max" size="30" type="number" />
                                </div>

                                <div>
                                        <label for="username">Cycle Time Min: </label>
                                        <input id="VI2_min" name="VI2_min" size="30" type="number" />
                                
                                        <label for="name">Cycle Time Max: </label>
                                        <input id="VI2_max" name="VI2_max" size="30" type="number" />
                                </div>

                                <div>
                                        <label for="username">Dwell Pressure Min: </label>
                                        <input id="VI3_min" name="VI3_min" size="30" type="number" />
                                
                                        <label for="name">Dwell Pressure Max: </label>
                                        <input id="VI3_max" name="VI3_max" size="30" type="number" />
                                </div>

                                <div>
                                        <label for="username">Ok ProdShot Min: </label>
                                        <input id="ok_prodShot_min" name="ok_prodShot_min" size="30" type="number" />
                                
                                        <label for="name">Ok ProdShot Max: </label>
                                        <input id="ok_prodShot_max" name="ok_prodShot_max" size="30" type="number" />
                                </div>

                                <div>
                                        <label for="username">Print Shot Min: </label>
                                        <input id="printShotMin_min" name="printShotMin_min" size="30" type="number" />
                                
                                        <label for="name">Print Shot Max: </label>
                                        <input id="printShotMin_max" name="printShotMin_max" size="30" type="number" />
                                </div>

                                <div>
                                        <label for="username">Filling Time Min: </label>
                                        <input id="fillingTime_min" name="fillingTime_min" size="30" type="number" />
                                
                                        <label for="name">Filling Time Max: </label>
                                        <input id="fillingTime_max" name="fillingTime_max" size="30" type="number" />
                                </div>

                                <div>
                                        <label for="username">Charging Time Min: </label>
                                        <input id="chargingTime_min" name="chargingTime_min" size="30" type="number" />
                                
                                        <label for="name">Charging Time Max: </label>
                                        <input id="chargingTime_max" name="chargingTime_max" size="30" type="number" />
                                </div>

                                <div>
                                        <label for="username">Take Out Time Min: </label>
                                        <input id="takeoutTime_min" name="takeoutTime_min" size="30" type="number" />
                                
                                        <label for="name">Take Out Time Max: </label>
                                        <input id="takeoutTime_max" name="takeoutTime_max" size="30" type="number" />
                                </div>

                                <div>
                                        <label for="username">Dwell Change Position Min: </label>
                                        <input id="dwellChnagePosition_min" name="dwellChnagePosition_min" size="30" type="number" />
                                
                                        <label for="name">Dwell Change Position Max: </label>
                                        <input id="dwellChnagePosition_max" name="dwellChnagePosition_max" size="30" type="number" />
                                </div>

                                <div>
                                        <label for="username">Minumum Cushion Position Min: </label>
                                        <input id="minumumCushionPosition_min" name="minumumCushionPosition_min" size="30" type="number" />
                                
                                        <label for="name">Minumum Cushion Position Max: </label>
                                        <input id="minumumCushionPosition_max" name="minumumCushionPosition_max" size="30" type="number" />
