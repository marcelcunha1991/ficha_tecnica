                                        <input id="crewRotationSpeed_min" name="crewRotationSpeed_min" size="30" type="number" />
                                
                                        <label for="name">Crew Rotation Speed Max: </label>
                                        <input id="crewRotationSpeed_max" name="crewRotationSpeed_max" size="30" type="number" />
                                </div>

                                                    
                            <br>
                            <button class="btn btn-primary">Cadastrar</button>
                        </form>
            </div>
           
          
    </div>
   
</div>

<script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
<%- include ('../partials/footer.ejs') %>