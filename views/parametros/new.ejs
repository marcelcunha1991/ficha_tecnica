                                <div>
                                        <label for="username">Injet Start Position Min: </label>
                                        <input id="injetStartPosition_min" name="injetStartPosition_min" size="30" type="number" />
                                
                                        <label for="name">Injet Start Position Max: </label>
                                        <input id="injetStartPosition_max" name="injetStartPosition_max" size="30" type="number" />
