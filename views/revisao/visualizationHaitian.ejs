<%- include ('../partials/header.ejs') %>
<%- include ('../partials/sidebar.ejs') %>

<div class="container-fluid">
   <br>
   <h2>Revisões</h2> 
   <a class="btn btn-success" href="/novaficha"> Inserir Nova Ficha Técnica</a>
   <a class="btn btn-success" href="/ficha/lista"> Lista de Fichas Técnicas</a>
   <button id="comparaButton" class="btn btn-warning" onclick="comparar()">Comparar Valores</button>
   <button id="editButton" class="btn btn-warning" onclick="editarRevisao()">Editar</button>
   <hr>

   <form method="GET" action="/ficha/lista" style="display: inline;">
      <button class="btn btn-info" style="width: 70px;">
         <i class="fa fa-arrow-left" title="Voltar"></i>
      </button>
   </form>
   <br>
   <br>
   
   <div class="card">
      <div class="card-header">                
         <h2>Revisão Id <%= injetor.revisao %></h2>              
      </div>
      <div class="card-body" id="visualização">
         <input type="hidden" class="read-only" readonly style="width: 100%" id="maquina" name="maquina" value="<%= injetor.maq %>">

         <div class="table-ficha">
            <table class="table-bordered table-responsive" style="width: 100%; margin-bottom: 40px;">
               <thead>
                  <tr>
                     <th>Número do Molde:</th>
                     <th>Número da Máquina:</th>
                     <th>Cliente:</th>
                     <th>Código PAM:</th>
                     <th>Técnico:</th>
                     <th>Produto:</th>
                     <th>Número do Material:</th>
                     <th>Revisão:</th>
                  </tr>
               </thead>
               <tbody>
                  <tr>
                     <td>
                        <input class="read-only" readonly style="width: 100%" id="molde" name="molde" value="<%= injetor.NúmeroMolde %>">
                     </td>
                     <td>
                        <input class="read-only" readonly style="width: 100%" id="numMaquina" name="numMaquina" value="<%= injetor.NúmeroMáquina %>">
                     </td>
                     <td>
                        <input class="read-only" readonly style="width: 100%" id="cliente" name="cliente" value="<%= injetor.Cliente %>">
                     </td>
                     <td>
                        <input class="read-only" readonly style="width: 100%" id="codigoPAM" name="codigoPAM" value="<%= injetor.CodigoPAM %>">
                     </td>
                     <td>
                        <input class="read-only" readonly style="width: 100%" id="tecnico" name="tecnico" value="<%= injetor.Tecnico %>">
                     </td>
                     <td>
                        <input class="read-only" readonly style="width: 100%" id="produto" name="produto" value="<%= injetor.Produto %>">
                     </td>
                     <td>
                        <p id="descMaterial"><%= injetor.Material %></p>
                     </td>
                     <td>
                        <input class="read-only" readonly style="width: 100%" id="revisao" name="revisao" value="<%= injetor.revisao %>">
                     </td>
                  </tr>
               </tbody>
            </table>
         </div>

         <div class="form-floating" style="margin-bottom: 50px;">
            <small>Usuário: <%= injetor.Usuario %></small><br>
            <label for="floatingTextarea">Motivo da alteração:</label>
            <textarea style="background-color: #fff;" readonly class="form-control" id="justificativa" name="justificativa"></textarea>
         </div>

