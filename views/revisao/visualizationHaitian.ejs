<%- include ('../partials/header.ejs') %>
<%- include ('../partials/sidebar.ejs') %>

<body onafterprint="removeClass()">
   <div class="container-fluid">
      <br class="dont-print">
      <h2 class="dont-print">Revisões</h2>
   
      <div class="d-flex">
         <a class="btn btn-success button-revisao dont-print" href="/novaficha"> Inserir Nova Ficha Técnica</a>
         <a class="btn btn-success button-revisao dont-print" href="/ficha/lista"> Lista de Fichas Técnicas</a>
         <button id="comparaButton" class="btn btn-warning button-revisao dont-print" onclick="comparar()">Comparar Valores</button>
         <button id="editButton" class="btn btn-warning dont-print" data-toggle='modal' data-target='#loginModal'>Editar</button>
         
         <select id="options" class="last-item dont-print">
            <option value="0">Opções</option>
            <option value="print">Imprimir</option>
            <option value="excel">Salvar em Excel</option>
         </select>
      </div>
      
      <hr class="dont-print">
   
      <form method="GET" action="/ficha/lista" style="display: inline;">
         <button class="btn btn-info dont-print" style="width: 70px;">
            <i class="fa fa-arrow-left" title="Voltar"></i>
         </button>
      </form>
      <br class="dont-print">
      <br class="dont-print">
      
      <div class="card">
         <div class="card-header">                
            <h2>Revisão Id <%= injetor.revisao %></h2>              
         </div>
         <div class="card-body" id="visualização">
            <input type="hidden" class="read-only" readonly style="width: 100%" id="maquina" name="maquina" value="<%= injetor.maq %>">
            <input type="hidden" class="read-only" readonly style="width: 100%" id="revisao" name="revisao" value="<%= injetor.revisao %>">
   
            <div class="table-ficha" style="width: fit-content; margin: 0 auto;">
               <table id="export_table_to_excel_1" class="table-bordered table-responsive header" style="width: 100%; margin-bottom: 40px;">
                  <thead>
                     <tr>
                        <th>Número do Molde:</th>
                        <th>Número da Máquina:</th>
                        <th>Cliente:</th>
                        <th>Código PAM:</th>
                        <th>Técnico:</th>
                        <th>Produto:</th>
                        <th>Número do Material:</th>
                        <th>Revisão:</th>
                     </tr>
                  </thead>
                  <tbody>
                     <tr>
                        <td>
                           <%= injetor.NúmeroMolde %>
                        </td>
                        <td>
                           <%= injetor.NúmeroMáquina %>
                        </td>
                        <td>
                           <%= injetor.Cliente %>
                        </td>
                        <td>
                           <%= injetor.CodigoPAM %>
                        </td>
                        <td>
                           <%= injetor.Tecnico %>
                        </td>
                        <td>
                           <%= injetor.Produto %>
                        </td>
                        <td>
                           <%= injetor.Material %>
                        </td>
                        <td>
                           <%= injetor.revisao %>
                        </td>
                     </tr>
                  </tbody>
               </table>
            </div>
   
            <div class="form-floating" style="margin-bottom: 50px;">
               <small id="ultimaEdicao">Última edição feita por: <%= injetor.Usuario %></small><br>
               <label for="floatingTextarea">Motivo da alteração:</label>
               <textarea style="background-color: #fff;" readonly class="form-control" id="justificativa" name="justificativa"></textarea>
            </div>
   
            <div class="d-flex justify-content-between ficha-container">
